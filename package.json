{
  "name": "react-emoji-selector",
  "version": "1.0.0",
  "description": "A React tool to select Emojis",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component --copy-files",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "nwb serve-react-demo",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "node-emoji": "^1.8.1",
    "node-sass-chokidar": "0.0.3"
  },
  "peerDependencies": {
    "react": "15.x"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.1",
    "nwb": "0.18.x",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tomrozendaal/react-emoji-selector.git"
  },
  "author": "Tom Rozendaal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomrozendaal/react-emoji-selector/issues"
  },
  "homepage": "https://github.com/tomrozendaal/react-emoji-selector",
  "keywords": [
    "react",
    "emoji",
    "selector",
    "picker",
    "emojis"
  ]
}
